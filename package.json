{
    "name": "paged-html",
    "version": "1.0.2",
    "description": "A library to render paged HTML which is print ready to any paper format",
    "main": "index.js",
    "scripts": {
        "watch": "cross-env NODE_ENV=development esbuild demo/public/index.js --bundle --minify=true --watch --format=esm --outfile=demo/dist/paged-html.js --servedir=demo",
        "build": "esbuild src/index.js --bundle --format=esm --minify --sourcemap=external --outfile=index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/1995navinkumar/paged-html.git"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "serve": "^11.1.0"
    },
    "dependencies": {
        "esbuild": "0.17.18"
    }
}