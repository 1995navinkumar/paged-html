{
    "name": "paged-html",
    "version": "1.0.0",
    "description": "A library to render paged HTML which is print ready to any paper format",
    "main": "index.js",
    "scripts": {
        "prepare": "npm run build",
        "build": "cross-env NODE_ENV=production rollup -c",
        "build-demo": "browserify demo/index.js -t babelify -t browserify-css -o demo/public/bundle.js",
        "start": "serve demo/public/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/1995navinkumar/paged-html.git"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.7.1",
        "@babel/runtime": "^7.7.2",
        "@rollup/plugin-replace": "^2.3.3",
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "browserify-css": "^0.15.0",
        "cross-env": "^6.0.3",
        "rollup": "^1.32.1",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-size": "^0.2.1",
        "rollup-plugin-size-snapshot": "^0.10.0",
        "rollup-plugin-terser": "^5.1.2",
        "serve": "^11.1.0"
    }
}